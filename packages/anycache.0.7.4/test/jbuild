(* -*- tuareg -*- *)

let coverage =
 try Sys.getenv "COVERAGE" = "1"
 with Not_found -> false

let optional_coverage =
  if coverage then {| (preprocess (pps (bisect_ppx))) |}
  else ""

let () =
  Printf.ksprintf Jbuild_plugin.V1.send {|
(jbuild_version 1)

(library
 ((name anycacheTest)
  %s
  (libraries (anycache alcotest))))
|} (optional_coverage)
