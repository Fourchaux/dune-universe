archetype effect_method_asset_add_collection

asset o_asset identified by oid {
  oid : string;
  val : int = 0;
} initialized by {
  {"oid";0}
}

asset my_asset identified by id {
  id     : string;
  value  : int;
  col    : o_asset collection;
} initialized by {
  {"id0"; 0; []};
  {"id1"; 1; []};
  {"id2"; 2; []}
}

variable res : int = 0

action exec () {
  effect {
    my_asset.get("id0").col.add({"oid";0})
  }
}

specification {
  s0: o_asset.count() = 0;
  s1: o_asset.sum(val) = 0;
}
