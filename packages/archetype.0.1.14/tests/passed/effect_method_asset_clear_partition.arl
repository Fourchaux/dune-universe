archetype effect_method_asset_clear_partition

asset o_asset identified by oid {
  oid : string;
  val : int;
}

asset my_asset identified by id {
  id     : string;
  value  : int;
  col    : o_asset partition;
} initialized by {
  {"id0"; 0; [{"oid"; 1}]};
  {"id1"; 1; []};
  {"id2"; 2; []}
}

variable res : int = 0

action exec () {
  effect {
    my_asset.get("id0").col.clear()
  }
}

specification {
  s0: o_asset.count() = 0;
  s1: o_asset.sum(val) = 0;
}
