archetype expr_method_asset_get_view

asset my_asset identified by id {
  id : string;
  value : int;
} initialized by {
  {"id0"; 0};
  {"id1"; 1};
  {"id2"; 2}
}

variable res : int = 0

action exec () {
  specification {
    s0: res = 1;
  }
  effect {
    var c = my_asset.select(value % 2 = 0);
    var a = c.get("id1");
    res := a.value
  }
}
