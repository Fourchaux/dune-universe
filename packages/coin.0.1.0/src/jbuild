(jbuild_version 1)

(rule
 ((targets (jbuild.inc.gen))
  (deps (generate/generate.ml (glob_files maps/*.TXT)))
  (action (run ${OCAML} ${<} --databases maps -o ${@}))))

(include jbuild.inc)

(alias
 ((name runtest)
  (action (diff jbuild.inc jbuild.inc.gen))))

(library
 ((name coin)
  (public_name coin)))
