(library
 (name dose_pef)
 (public_name dose3.pef)
 (libraries extlib cudf dose3.common dose3.versioning)
 (flags (:standard)))

(ocamllex packages_lexer)

(ocamlyacc packages_parser)

(rule
 (alias runtest)
 (deps ../../tests/pef/unittests.pef)
 (action
  (chdir
   ../../
   (run ./src/pef/tests/tests.exe))))
