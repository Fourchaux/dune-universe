univ : {a b c a1 a2 a3};

var event : {} { a1 a2 a3 };

const A : {a};
const B : {b};
const C : {c};

const e1 : { a1};
const e2 : { a2};
const e3 : { a3};

var s : { } {a b c};

run
always one event;

always {
			 event = e1 => A in s' else
			 event = e2 => B in s' else
			 event = e3 => C in s';
};

sometime (no s and next sometime #s = 3)