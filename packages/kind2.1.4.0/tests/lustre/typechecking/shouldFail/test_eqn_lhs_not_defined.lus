node TriplexVoter(alt1,alt2,alt3: real) returns (r: real);
  var ad12,ad13,ad23: real;
let
  ad12 = Abs(alt1-alt2);
  ad13 = Abs(alt1-alt3);
  ad23 = Abs(alt2-alt3);
  m = Min(ad12, Min(ad13, ad23));
  r = if m=ad12 then (alt1+alt2)/2.0
      else if m=ad13 then (alt1+alt3)/2.0
      else (alt2+alt3)/2.0;
tel
