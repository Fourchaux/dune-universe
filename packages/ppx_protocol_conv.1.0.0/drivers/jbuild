(jbuild_version 1)

(library
 ((name protocol_conv_json)
  (public_name ppx_protocol_conv.json)
  (modules (Json))
  (flags (:standard -w -3))
  (libraries (protocol_conv base yojson))
  (synopsis "json (de)serialization driver for ppx_protocol_conv")
 )
)

(library
 ((name protocol_conv_xml)
  (public_name ppx_protocol_conv.xml)
  (modules (Xml_light))
  (flags (:standard -w -3))
  (libraries (protocol_conv base xml-light))
  (synopsis "xml-light (de)serialization driver for ppx_protocol_conv")
 )
)

(library
 ((name protocol_conv_msgpack)
  (public_name ppx_protocol_conv.msgpack)
  (modules (msgpack))
  (flags (:standard -w -3))
  (libraries (protocol_conv base msgpck))
  (synopsis "MessagePack (de)serialization driver for ppx_protocol_conv")
 )
)
